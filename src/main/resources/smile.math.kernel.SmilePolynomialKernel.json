{
  "name": "SmilePolynomialKernel",
  "package": "smile.math.kernel",
  "prefix": "",
  "suffix": "Base",
  "superclass": "smile.math.kernel.AbstractSmileKernel<smile.math.kernel.PolynomialKernel>",
  "implement": [],
  "author": "FracPete",
  "organization": "University of Waikato, Hamilton, NZ",
  "options": [
    {
      "property": "degree",
      "type": "int",
      "default": "2",
      "constraint": "value > 0",
      "help": "the polynomial degree."
    },
    {
      "property": "scale",
      "type": "double",
      "default": "1.0",
