{
  "name": "SmileKMeans",
  "package": "smile.clustering",
  "prefix": "",
  "suffix": "Base",
  "superclass": "smile.clustering.AbstractSmileClusterer",
  "implement": [],
  "author": "FracPete",
  "organization": "University of Waikato, Hamilton, NZ",
  "options": [
    {
      "property": "numClusters",
      "type": "int",
      "default": "2",
      "constraint": "value >= 2",
      "help": "The number of clusters to determine."
    },
    {
      "property": "maxIter",
      "type": "int",
      "default": "100",
      "constraint": "value >= 1",
      "help": "The maximum number of iterations in each run."
    },
    {
      "property": "runs",
      "type": "int",
      "constraint": "value >= 1",
      "default": "1",
      "help": "The number of runs; if more than 1 then the best model is used."
    }
  ]
}
