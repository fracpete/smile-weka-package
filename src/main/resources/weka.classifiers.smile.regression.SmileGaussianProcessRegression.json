{
  "name": "SmileGaussianProcessRegression",
  "package": "weka.classifiers.smile.regression",
  "prefix": "",
  "suffix": "Base",
  "superclass": "weka.classifiers.smile.AbstractSmileRegressor",
  "implement": [],
  "author": "FracPete",
  "organization": "University of Waikato, Hamilton, NZ",
  "options": [
    {
      "property": "kernel",
      "type": "weka.classifiers.smile.math.kernel.AbstractSmileKernel",
      "default": "new weka.classifiers.smile.math.kernel.SmileGaussianKernel()",
      "help": "The kernel to use."
    },
    {
      "property": "lambda",
      "type": "double",
      "constraint": "value >= 0.0",
      "default": "0.01",
      "help": "The shrinkage/regularization factor."
    }
  ]
}
