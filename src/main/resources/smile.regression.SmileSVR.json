{
  "name": "SmileSVR",
  "package": "smile.regression",
  "prefix": "",
  "suffix": "Base",
  "superclass": "smile.regression.AbstractSmileRegressor",
  "implement": [],
  "author": "FracPete",
  "organization": "University of Waikato, Hamilton, NZ",
  "options": [
    {
      "property": "kernel",
      "type": "smile.math.kernel.AbstractSmileKernel",
      "default": "new smile.math.kernel.SmileGaussianKernel()",
      "help": "The kernel to use."
    },
    {
      "property": "epsilon",
      "type": "double",
      "constraint": "value >= 0.0",
      "default": "1e-3",
      "help": "The epsilon parameter."
    },
    {
      "property": "capacity",
      "type": "double",
      "constraint": "value >= 0.0",
      "default": "1.0",
      "help": "The capacity parameter."
    },
    {
      "property": "tolerance",
      "type": "double",
      "constraint": "value > 0.0",
      "default": "0.001",
      "help": "The convergence tolerance."
    }
  ]
}
